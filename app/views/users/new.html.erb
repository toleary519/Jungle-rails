<h2> Welcome! Please enter your information to signup!<h2>

<div class="order-summary jumbotron">
<%= form_for :user, url: '/users' do |form|  %>
  First Name: <%= form.text_field :first_name %>
  
  Last Name: <%= form.text_field :last_name %>
  
  Email: <%= form.text_field :email %>
  
  Password: <%= form.password_field :password %>
  
  Password Confirmation : <%= form.password_field :password_confirmation %>
  
  <%= form.submit "Submit" %>
  
  <% end %>
</div>